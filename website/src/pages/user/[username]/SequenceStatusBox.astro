---
const { count, description } = Astro.props;
---

<div class='collapse bg-base-200'>
    <input type='checkbox' />
    <div class='collapse-title text-xl font-medium'>
        {count} sequence(s) {description}
    </div>
    <div class='collapse-content'>
        {count > 0 && <slot />}
    </div>
</div>
